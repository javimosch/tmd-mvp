    "@nuxtjs/pwa": "^2.0.5",
    "add": "^2.0.6",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "bootstrap-vue": "^2.0.0-rc.1",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "error-to-json": "^1.1.0",
    "git-directory-deploy": "^1.5.1",
    "gsap": "^1.20.4",
    "html2pdf.js": "^0.8.2",
    "iron-node": "^3.0.20",
    "jquery": "^3.3.1",
    "jquery-circle-progress": "^1.2.2",
    "js-base64": "^2.4.3",
    "js-beautify": "^1.7.5",
    "js-cookie": "^2.2.0",
    "jsonexport": "^2.0.11",
    "localforage": "^1.6.0",
