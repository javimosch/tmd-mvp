{
  "name": "lti-nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "javimosch <arancibiajav@gmail.com>",
  "private": true,
  "scripts": {
    "test": "npx tape 'tests/**/*.js' | tap-notify | tap-summary",
    "devv": "npx run-p dev:nuxt dev:generator",
    "dev": "yarn dev:nuxt",
    "dev:nuxt": "cross-env HOST=0.0.0.0 PORT=3005 nuxt",
    "dev:generator": "npx nodemon --exec npx babel-node generator/index.js --watch assets --watch pages/index.vue --watch components --watch generator -e js,json,vue",
    "prod:generator": "babel-node generator/index.js",
    "build": "nuxt build",
    "heroku-postbuild": "npm run build",
    "start": "nuxt start",
    "generate": "yarn prod:generator && cross-env NODE_ENV=production nuxt generate",
    "deploy": "git-directory-deploy --directory dist --branch master",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-brands": "^5.0.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
